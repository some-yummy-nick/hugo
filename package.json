{
	"name": "hugo-template",
	"description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
	"repository": "netlify/victor-hugo",
	"scripts": {
		"hugo": "gulp hugo",
		"prod": "gulp build",
		"prod-preview": "gulp build-preview",
		"initial": "gulp server",
		"initial-preview": "gulp server-preview",
		"dev-watch": "webpack --watch --env=new --mode=production",
		"start": "npm-run-all --parallel initial dev-watch",
		"start-preview": "npm-run-all --parallel initial-preview dev-watch",
		"webpack": "webpack --env=old --mode=production",
		"dev": "webpack --env=new --mode=production",
		"build": "npm-run-all --parallel prod webpack dev",
		"build-preview": "npm-run-all --parallel prod-preview webpack dev",
		"lint": "eslint src",
		"deploy": "gh-pages -d dist"
	},
	"author": "Marat",
	"license": "MIT",
	"dependencies": {
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-syntax-object-rest-spread": "^6.13.0",
		"babel-plugin-transform-class-properties": "^6.10.2",
		"babel-plugin-transform-object-assign": "^6.8.0",
		"babel-plugin-transform-object-rest-spread": "^6.8.0",
		"babel-register": "^6.26.0",
		"browser-sync": "^2.26.3",
		"css-loader": "^0.28.11",
		"eslint": "^4.19.1",
		"eslint-plugin-import": "^2.15.0",
		"exports-loader": "^0.7.0",
		"file-loader": "^1.1.11",
		"gh-pages": "^2.0.1",
		"gulp": "3.9.0",
		"gulp-babel": "^7.0.1",
		"gulp-flatten": "^0.4.0",
		"gulp-util": "^3.0.7",
		"gulp-watch": "^5.0.1",
		"hugo-bin": "^0.29.0",
		"imports-loader": "^0.8.0",
		"path": "^0.12.7",
		"postcss-cssnext": "^3.1.0",
		"postcss-import": "^11.1.0",
		"postcss-nested": "^3.0.0",
		"siema": "^1.5.1",
		"url-loader": "^1.1.2",
		"webpack": "4.6.0",
		"whatwg-fetch": "^2.0.4"
	},
	"browserslist": [
		"> 0.01%",
		"IE 10",
		"last 2 versions"
	],
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/polyfill": "^7.2.5",
		"@babel/preset-env": "^7.3.1",
		"@babel/register": "^7.0.0",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"css-declaration-sorter": "^4.0.1",
		"css-mqpacker": "^7.0.0",
		"gulp-autoprefixer": "^6.0.0",
		"gulp-clean-css": "^4.0.0",
		"gulp-load-plugins": "^1.5.0",
		"gulp-postcss": "^7.0.1",
		"gulp-sass": "^4.0.2",
		"gulp-sass-glob": "^1.0.9",
		"npm-run-all": "^4.1.5",
		"postcss-easysprites": "^0.1.9",
		"webpack-cli": "^3.2.1",
		"webpack-merge": "^4.2.1"
	}
}
